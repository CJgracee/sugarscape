set title "Agent Wealth Total Divided by Population"
set xlabel "Timestep"
set ylabel "Wealth total / Population"
set lt 1 lw 2 lc "black"
set key fixed right top
set xtics nomirror
set ytics nomirror
set term pdf
set output "agentWealthTotalDivByPop.pdf"
plot ARGV[1] using 'timestep':'agentWealthTotalDivByPop-benthamHalfLookaheadBinary' with linespoints pointinterval 100 pointsize 0.75 lt 1 dt 1 pt 0 title 'benthamHalfLookaheadBinary', \
    ARGV[1] using 'timestep':'agentWealthTotalDivByPop-benthamHalfLookaheadTop' with linespoints pointinterval 100 pointsize 0.75 lt 1 dt 2 pt 1 title 'benthamHalfLookaheadTop', \
    ARGV[1] using 'timestep':'agentWealthTotalDivByPop-benthamNoLookaheadBinary' with linespoints pointinterval 100 pointsize 0.75 lt 1 dt 3 pt 2 title 'benthamNoLookaheadBinary', \
    ARGV[1] using 'timestep':'agentWealthTotalDivByPop-egoisticHalfLookaheadTop' with linespoints pointinterval 100 pointsize 0.75 lt 1 dt 4 pt 3 title 'egoisticHalfLookaheadTop', \
    ARGV[1] using 'timestep':'agentWealthTotalDivByPop-rawSugarscape' with linespoints pointinterval 100 pointsize 0.75 lt 1 dt 5 pt 4 title 'rawSugarscape'